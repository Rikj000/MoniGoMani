  "order_types": {
    "buy": "limit",
    "sell": "limit",
    "stoploss": "market",
    "stoploss_on_exchange": false
  },
  "order_time_in_force": {
    "buy": "gtc",
    "sell": "gtc"
  },
  "exchange": {
    "ccxt_config": {
      "enableRateLimit": true
    },
    "ccxt_async_config": {
      "enableRateLimit": true,
      "rateLimit": 200
    },
    "pair_blacklist": [
      ".*USD/USDT",
      "USD.*/USDT",
      ".*DOWN/BTC",
      ".*UP/BTC",
      ".*DOWN/ETH",
      ".*UP/ETH",
      ".*DOWN/USDT",
      ".*UP/USDT",
      ".*DOWN/BNB",
      ".*UP/BNB",
      ".*/BNB",
      "BNB/.*",
      ".*_PREMIUM",
      ".*PERP",
      ".*BULL/.*",
      ".*BEAR/.*",
      ".*BULL2021/.*",
      ".*BEAR2021/.*"
